    <title>Property</title>

    <!-- google fonts -->
    <link href="http://fonts.googleapis.com/css2?family=Kumbh+Sans:wght@300;400;700&amp;display=swap" rel="stylesheet">
    <link href="~/assets/css/style-liberty.css" rel="stylesheet" />
