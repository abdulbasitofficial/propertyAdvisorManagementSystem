<body>


    <!--header-->
    <header id="site-header" class="fixed-top">
        <div class="container">
            <nav class="navbar navbar-expand-lg stroke px-0">
                <h1>
                    <a class="navbar-brand" href="@Url.Action("Index","Home")">
                        <span class="fa fa-home"></span> Property @Session["Type"]
                    </a>
                </h1>
                <!-- if logo is image enable this
                <a class="navbar-brand" href="#index.html">
                    <img src="image-path" alt="Your logo" title="Your logo" style="height:35px;" />
                </a> -->
                <button class="navbar-toggler  collapsed bg-gradient" type="button" data-toggle="collapse"
                        data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false"
                        aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon fa icon-expand fa-bars"></span>
                    <span class="navbar-toggler-icon fa icon-close fa-times"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
                    <ul class="navbar-nav ml-lg-5 mr-auto">

                        @if (Session["Email"] != null)
                        {
                            <li class="nav-item active">
                                <a class="nav-link" href="@Url.Action("Index","Home")">Home <span class="sr-only">(current)</span></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Property","Property")">Property</a>
                            </li>
                            if (Session["Type"].Equals("Advisor"))
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Company","Company")">Company</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Refer","Home")">Refered</a>
                                </li>
                            }
                            if (Session["Type"].Equals("Buyer") ||Session["Type"].Equals("Seller") )
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("CompanyUserSide", "Company")">Company</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Advisor","Home")">Advisor</a>
                                </li>
                            }

                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Notification","Home")">Notification</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Offer","Property")">Offer</a>
                            </li>

                        }
                    </ul>
                    <div class="top-quote mt-lg-0">
                        @if (Session["Email"] != null)
                        {
                        <a href="@Url.Action("LogOut","Account")" class="btn btn-style btn-primary">
                            ( @Session["Name"] ) <span class="fa fa-lock"></span> LogOut
                        </a>
                        }
                        else
                        {
                            <a href="@Url.Action("Login","Account")" class="btn btn-style btn-primary">
                                <span class="fa fa-lock"></span> LogIn
                            </a>
                        }

                    </div>
                    <!--/search-right-->
                    <!--//search-right-->
                </div>

                <!-- toggle switch for light and dark theme -->
                <!-- //toggle switch for light and dark theme -->
            </nav>
        </div>
    </header>
    <!--/header-->
    <section class="w3l-cover-3">
        <div class="cover top-bottom">
            <div class="container">
                <div class="middle-section text-center" >
                    <div class="section-width" align="center">
                        <h2>Find a property today</h2>

                        <form action="@Url.Action("Search","Home")" class="w3l-cover-3-gd" method="post">

                            <span class="input-group-btn" style="width:220px;margin-right:5px;"  align="center">
                                <select class="btn btn-default" name="type" required>
                                    <option value="Home">Home</option>
                                    <option value="Land">Land</option>
                                    <option value="Office">Office</option>
                                    <option value="Appartment">Appartment</option>
                                </select>
                            </span>
                            <span class="input-group-btn" style="width:220px;"  align="center">
                                <select class="btn btn-default" name="location" required>
                                    <option value="Islamabad">Islamabad</option>
                                    <option value="Faisalabad">Faisalabad</option>
                                    <option value="Rawalpindi">Rawalpindi</option>
                                    <option value="Multan">Multan</option>
                                    <option value="Peshawar">Peshawar</option>
                                    <option value="Lahore">Lahore</option>
                                    <option value="Karachi">Karachi</option>
                                    <option value="Kashmir">Kashmir</option>
                                </select>
                            </span>
                            <table align="center" >
                                <tr>
                                    <td>
                                        <input type="number" name="from" placeholder="From" style="height:70px;width:180px;" required>
                                    </td>
                                    <td>
                                        <input type="number" name="to" placeholder="To" style="height:70px;width:180px;" required>
                                    </td>
                                </tr>
                            </table>


                            <button type="submit" class="btn-primary">Search</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="container">
        <br />
        @RenderBody()

        <br /><br /><br />
    </div>
    <!-- jQuery and Bootstrap JS -->
    <script src="~/assets/js/jquery-3.3.1.min.js"></script>

    <script src="~/assets/js/theme-change.js"></script><!-- theme switch js (light and dark)-->
    <!-- stats number counter-->
    <script src="~/assets/js/jquery.waypoints.min.js"></script>
    <script src="~/assets/js/jquery.countup.js"></script>
    <script>
  $('.counter').countUp();
    </script>
    <!-- //stats number counter -->
